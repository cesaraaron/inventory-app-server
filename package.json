{
  "name": "my-app-server",
  "scripts": {
    "start": "dotenv -- nodemon -e ts,graphql -x ts-node src/index.ts",
    "dev": "npm-run-all --parallel start playground",
    "debug": "dotenv -- nodemon -e ts,graphql -x ts-node --inspect src/index.ts",
    "playground": "graphql playground",
    "build": "rimraf dist && tsc",
    "prepare": "graphql get-schema -p database && graphql codegen",
    "deploy": "prisma deploy"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "dotenv": "^6.0.0",
    "expo-server-sdk": "^2.3.3",
    "graphql-yoga": "^1.17.4",
    "jsonwebtoken": "8.2.1",
    "prisma-binding": "2.3.10"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.1",
    "@types/jsonwebtoken": "^7.2.7",
    "dotenv-cli": "1.4.0",
    "graphql-cli": "^3.0.11",
    "nodemon": "1.17.4",
    "npm-run-all": "4.1.3",
    "prisma": "^1.32.0-beta",
    "rimraf": "2.6.2",
    "ts-node": "6.0.3",
    "typescript": "2.8.3"
  },
  "resolutions": {
    "**/event-stream": "^4.0.1",
    "graphql": "0.13.1"
  }
}
